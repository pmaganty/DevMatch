  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.8.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.5.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "react-bootstrap": "^1.0.1",
    "react-overlays": "^3.1.3",
    "react-signup-login-component": "^1.2.0",
    "styled-components": "^5.1.0"
  }
}
